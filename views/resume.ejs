<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Resume</title>
</head>
<body>
    <h1><%= name %>'s Resume</h1>
    <p>Email: <%= email %></p>
    <p>Phone: <%= phone %></p>
    <h2>Experience</h2>
    <ul>
        <% experience.forEach(function(job) { %>
            <li><%= job %></li>
        <% }); %>
    </ul>
</body>
</html>
